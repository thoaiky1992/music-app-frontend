{
  "name": "app-mp3-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "docker:build": "docker build -t react-nginx . && docker run -d --name react_app --rm -it -p 3000:80 react-nginx",
    "docker:stop_container": "docker container stop react_app",
    "docker:remove_container": "docker container rm react_app",
    "docker:remove_image": "docker rmi react-nginx",
    "docker:deploy": "yarn docker:stop_container || true && yarn docker:remove_container || true && yarn docker:remove_image && yarn docker:build"
  },
  "dependencies": {
    "@headlessui/react": "^1.6.6",
    "@heroicons/react": "^1.0.6",
    "@types/node": "^18.0.6",
    "animate.css": "^4.1.1",
    "axios": "^0.27.2",
    "classnames": "^2.3.1",
    "formik": "^2.2.9",
    "history": "^5.3.0",
    "path": "^0.12.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.4.0",
    "react-redux": "^8.0.2",
    "react-responsive": "^9.0.0-beta.10",
    "react-router": "^6.3.0",
    "react-router-dom": "^6.3.0",
    "redux": "^4.2.0",
    "redux-thunk": "^2.4.1",
    "socket.io-client": "^4.5.1",
    "swiper": "^8.3.2",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/line-clamp": "^0.4.0",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/react-redux": "^7.1.24",
    "@vitejs/plugin-react": "^2.0.0",
    "autoprefixer": "^10.4.7",
    "postcss": "^8.4.14",
    "sass": "^1.54.0",
    "tailwindcss": "^3.1.6",
    "tailwindcss-labeled-groups": "^0.0.2",
    "typescript": "^4.6.4",
    "vite": "^3.0.0"
  }
}
